{
  "name": "farmbuild-web-services-demo",
  "private": true,
  "version": "0.1.0",
  "description": "This project is created to demonstrate the Farm Build WFS services",
  "repository": "https://github.com/FarmBuild/farmbuild-web-services-demo",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.3.1",
    "expect.js": "0.3.1",
    "http-server": "^0.6.1",
    "jshint": "2.6.3",
    "minami": "~1.1.0",
    "mocha": "^2.2.4",
    "ncp": "2.0.0",
    "shelljs": "^0.2.6",
    "superagent": "1.2.0"
  },
  "scripts": {
    "postinstall": "",
    "prestart": "npm install",
    "start": "http-server -a localhost -p 8000 -c-1",
    "pretest": "npm install",
    "test": "mocha --recursive examples/angularjs/**/*.spec.js",
    "test-single-run": "karma start karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor protractor.conf.js",
    "clean": "rm -rf dist/* & rm -rf docs/*",
    "generate-docs": "node_modules/.bin/jsdoc --configure .jsdoc.json"
  }
}
